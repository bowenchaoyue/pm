<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN"   "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
    <!-- 实体类,简称 -设置别名 -->  
    <typeAliases>  
        <!--package name="com.iflytek.xtzk.**.domain"/-->
        <typeAlias alias="messageVO" type="com.chaowen.model.vo.MessageVO"/>
        <typeAlias alias="productVO" type="com.chaowen.model.vo.ProductVO"/>
        <typeAlias alias="messageDTO" type="com.chaowen.model.dto.MessageDTO"/>
        <typeAlias alias="productDTO" type="com.chaowen.model.dto.ProductDTO"/>
        <typeAlias alias="infomationVO" type="com.chaowen.model.vo.InfomationVO"/>
        <typeAlias alias="infomationDTO" type="com.chaowen.model.dto.InfomationDTO"/>
        <typeAlias alias="category" type="com.chaowen.model.Category"/>
        <typeAlias alias="dictionary" type="com.chaowen.model.Dictionary"/>
        <typeAlias alias="team" type="com.chaowen.model.Team"/>
        <typeAlias alias="user" type="com.chaowen.model.User"/>
        <typeAlias alias="cooperation" type="com.chaowen.model.Cooperation"/>
        <typeAlias alias="picture" type="com.chaowen.model.Picture"/>
        <typeAlias alias="series" type="com.chaowen.model.Series"/>
    </typeAliases>

   <plugins>
    <!-- com.github.pagehelper为PageHelper类所在包名 -->
    <plugin interceptor="com.github.pagehelper.PageHelper">
     <property name="dialect" value="mysql"/>
    </plugin>
   </plugins>
</configuration>